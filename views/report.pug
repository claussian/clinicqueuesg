extends layout

block content
  nav.navbar.navbar-default.navbar-static-top
    .container-fluid
      .navbar-header
        a.navbar-brand(href='/') ClinicQueues
          span.sg SG
      #bs-example-navbar-collapse-1.collapse.navbar-collapse
        .nav.navbar-nav.navbar-right
          p
            | Queue report
  #map-container.container-fluid
    .row
      #map.col-md-8
      .col-md-4.queue-report
        .container-fluid.form-container
          .row.well.well-lg
            form.form-horizontal
              fieldset
                legend.legend-container Submit a clinic queue report!
                label.control-label(for='inputPassword') Clinic name
                form.form-group
                  .typeahead__container
                    .typeahead__field
                      span.typeahead__query
                        input.js-typeahead(name='q', type='search', autocomplete='off')
                      span.typeahead__button
                        button(type='submit')
                          span.typeahead__search-icon
                .form-group
                  label.control-label(for='inputPassword') Password
                    input#inputPhoto.form-control(type='image', placeholder='Password', name='password')
                .form-group
                  .col-lg-12
                    button.btn.btn-default.btn-form(type='reset') Cancel
                    button.btn.btn-primary.btn-form(type='submit') Submit
          
        
